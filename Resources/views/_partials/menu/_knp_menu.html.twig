{% extends 'knp_menu.html.twig' %}
{% block label %}
    {% if item.getExtra('label-icon') is defined %}
        <i class="{{ item.getExtra('label-icon') }}"></i>
    {% endif %}
    {{ item.label|trans(
    item.getExtra('translation_params', {}),
    item.getExtra('translation_domain', 'admninInterface')
    ) |capitalize}}
{% endblock %}
